{
  "compilerOptions": {
    "strict": true,
    "alwaysStrict": true,
    "outDir": "./dist",
    "rootDir": ".",
    "baseUrl": "./src",
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "allowUnreachableCode": false,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "noUnusedParameters": true,
    "strictPropertyInitialization": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "newLine": "LF",
    "paths": {
      "@manager": [
        "manager/index"
      ],
      "@common/*": [
        "common/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@components": [
        "components/index"
      ],
      "@bot": [
        "bot"
      ]
    }
  }
}